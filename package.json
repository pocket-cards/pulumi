{
  "name": "pocket-cards",
  "devDependencies": {
    "@types/node": "^13.13.4"
  },
  "scripts": {
    "start": "pulumi up -e -y -p 100 --skip-preview",
    "start:install": "cross-env ENVIRONMENT=local pulumi up -e -y -p 100 --skip-preview",
    "start:attach": "cross-env ENVIRONMENT=initialize pulumi up -e -y -p 100 --skip-preview",
    "build": "pulumi up -p 100",
    "valid": "tsc --noemit",
    "diff": "pulumi up --diff ",
    "diff:install": "cross-env ENVIRONMENT=local pulumi up --diff ",
    "show:output": "pulumi stack ouput -j",
    "secret": "pulumi config set --secret ZONE_ID Z103238422UEIND0A9CSR",
    "state:import": "pulumi stack import --file state.json",
    "state:export": "pulumi stack export --file state.json"
  },
  "dependencies": {
    "@pulumi/aws": "^2.2.0",
    "@pulumi/awsx": "^0.20.0",
    "@pulumi/pulumi": "^2.1.0",
    "cross-env": "^7.0.2"
  }
}
